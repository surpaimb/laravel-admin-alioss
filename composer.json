{
  "name": "surpaimb/laravel-admin-alioss",
  "description": "扩展laravel-admin的form表单，使其可以使用web直传阿里OSS",
  "type": "library",
  "keywords": [
    "laravel-admin",
    "extension",
    "alioss"
  ],
  "homepage": "https://github.com/surpaimb/laravel-admin-alioss.git",
  "license": "MIT",
  "authors": [
    {
      "name": "surpaimb",
      "email": "surpaimb@163.com"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "aliyuncs/oss-sdk-php": "~2.0",
    "encore/laravel-admin": "~1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Surpaimb\\AliOssUpload\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Surpaimb\\AliOssUpload\\AliOssUploadServiceProvider"
      ]
    }
  }
}
